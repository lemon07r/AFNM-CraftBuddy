{
  "name": "afnm-craftbuddy",
  "version": "2.3.4",
  "description": "Automatically calculates and displays the optimal next skill to use during crafting in Ascend From Nine Mountains",
  "author": {
    "name": "lemon07r"
  },
  "main": "dist/mod.js",
  "scripts": {
    "build": "bun run webpack --mode production",
    "postbuild": "bun scripts/zip-dist.js",
    "test": "bun run jest",
    "test:watch": "bun run jest --watch",
    "test:coverage": "bun run jest --coverage",
    "docs:check-links": "bun run scripts/docs/check-links.js",
    "docs:check-freshness": "bun run scripts/docs/check-freshness.js",
    "docs:check-authority": "bun run scripts/docs/check-authority.js",
    "docs:check": "bun run docs:check-links && bun run docs:check-freshness && bun run docs:check-authority",
    "docs:inventory": "bun run scripts/docs/generate-inventory.js"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react-dom": "^19.2.3",
    "afnm-types": "0.6.38",
    "file-loader": "^6.2.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1",
    "zip-a-folder": "^4.0.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@reduxjs/toolkit": "^2.11.2",
    "react": "^19.2.4"
  }
}
